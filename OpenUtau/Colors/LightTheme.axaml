<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <system:Boolean x:Key="IsDarkMode">false</system:Boolean>

  <Color x:Key="BackgroundColor">#FFFFFF</Color>
  <Color x:Key="BackgroundColorPointerOver">#F5F0FA</Color>
  <Color x:Key="BackgroundColorPressed">#EDE6F8</Color>
  <Color x:Key="BackgroundColorDisabled">#F0F0F0</Color>

  <Color x:Key="ForegroundColor">#2E1A3D</Color>
  <Color x:Key="ForegroundColorPointerOver">#3B2452</Color>
  <Color x:Key="ForegroundColorPressed">#4A3368</Color>
  <Color x:Key="ForegroundColorDisabled">#A399B0</Color>

  <Color x:Key="BorderColor">#BFA0FF</Color>
  <Color x:Key="BorderColorPointerOver">#D9B3FF</Color>

  <Color x:Key="SystemAccentColor">#A366FF</Color>
  <Color x:Key="SystemAccentColorLight1">#C299FF</Color>
  <Color x:Key="SystemAccentColorDark1">#7D33CC</Color>

  <Color x:Key="NeutralAccentColor">#C0B8CC</Color>
  <Color x:Key="NeutralAccentColorPointerOver">#A890B8</Color>

  <Color x:Key="AccentColor1">#A366FF</Color>
  <Color x:Key="AccentColor2">#FF66D9</Color>
  <Color x:Key="AccentColor3">#302932</Color>
  <Color x:Key="WarningColor">#FFF4CE</Color>

  <!--Keyboard Background-->
  <Color x:Key="WhiteKeyColorLeft">Transparent</Color>
  <Color x:Key="WhiteKeyColorRight">Transparent</Color>
  <Color x:Key="WhiteKeyNameColor">#FF347c</Color>

  <Color x:Key="TickLineColor">#BFA0FF</Color>
  <Color x:Key="BarNumberColor">#A890B8</Color>
  <Color x:Key="FinalPitchColor">#C0A0D0</Color>
  <Color x:Key="TrackBackgroundAltColor">#F5F0FA</Color>

  <Color x:Key="WhiteKeyColorLeft">#F0E6FA</Color>
  <Color x:Key="WhiteKeyColorRight">#E6D6F8</Color>
  <Color x:Key="WhiteKeyNameColor">#4A1A80</Color>

  <Color x:Key="CenterKeyColorLeft">#D8B3FF</Color>
  <Color x:Key="CenterKeyColorRight">#C299FF</Color>
  <Color x:Key="CenterKeyNameColor">#4A1A80</Color>

  <Color x:Key="BlackKeyColorLeft">#B366FF</Color>
  <Color x:Key="BlackKeyColorRight">#8E33CC</Color>
  <Color x:Key="BlackKeyNameColor">#FFFFFF</Color>
</ResourceDictionary>
